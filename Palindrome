def reverse(s: str) -> str:
    """Return reverse on the string given
    
    >>> reverse('noon')
    'noon'
    >>> reverse('dented')
    'detned'
    
    """
    word = ''
    
    for i in s:
        word = i + word
        
    return word
    
def palindrome(s: str) -> bool:
    """return is palindrome or not
    
    >>>palindrome('noon')
    True
    >>> palindrome('dented')
    False
    """
    
    return reverse(s) == s
